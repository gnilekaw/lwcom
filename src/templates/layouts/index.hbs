<!doctype html>
<html class="no-js" lang="en">
{{> head }}
<body>
<!--[if lt IE 8]>
    <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
<![endif]-->

<!-- Subhead -->
<header class="masthead subhead">
  <div class="container">
    <div class="row">
      <div class="col col-lg-12">
        <h1>{{ title }}</h1>
      </div>
    </div>
  </div>
</header>

<!-- Page content -->
<div class="container">
<div class="panel panel-docs panel-clear">
  {{#markdown}}
    {{> body }}
  {{/markdown}}
</div>
</div>

<div class="more-work"></div>

<!-- scripts -->
{{> scripts }}

<!-- auto loader -->
{{> infinite-loader }}

<!-- Analytics -->
{{> analytics }}

</body>
</html>