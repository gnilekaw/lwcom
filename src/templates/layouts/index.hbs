<!DOCTYPE html>
<html lang="en">

{{> head }}

<body>

<!-- Subhead -->
<header class="masthead subhead">
  <div class="container">
    <div class="row">
      <div class="col col-lg-12">
        <h1>{{ uppercase title }}</h1>
      </div>
    </div>
  </div>
</header>

<!-- Page content -->
<div class="container">
<div class="panel panel-docs panel-clear">
  {{#markdown}}
    {{> body }}
  {{/markdown}}
</div>
</div>

<div class="more-work"></div>

<!-- scripts -->
{{> scripts }}

<!-- auto loader -->
{{> infinite-loader }}

<!-- Analytics -->
{{> analytics }}

</body>
</html>